            <targetProfile value="http://hl7.org/fhir/StructureDefinition/Substance"></targetProfile>
            <targetProfile value="https://fhir.nhs.uk/R4/StructureDefinition/UKCore-Medication"></targetProfile>
         </type>
      </element>
